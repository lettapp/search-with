{
    "manifest_version": 3,
    "minimum_chrome_version": "100",
    "version": "24.9.11",
    "name": "Lett Search With",
    "description": "Search for selected text with any engine.",
    "icons": {
        "32": "/assets/icon32.png",
        "128": "/assets/icon.png"
    },
    "permissions": [
        "storage",
        "contextMenus",
        "scripting",
        "activeTab"
    ],
    "background": {
        "service_worker": "/js/worker.js"
    },
    "omnibox": {
        "keyword": "lett"
    },
    "commands": {
        "Prompt": {
            "suggested_key": {
                "default": "Ctrl+S"
            },
            "description": "Prompt Search"
        }
    },
    "action": {}
}